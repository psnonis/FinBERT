name: "preprocess_resnet50_ensemble"
platform: "ensemble"
max_batch_size: 128
input [
  {
    name: "INPUT"
    data_type: TYPE_STRING
    dims: [ 1 ]
  }
]
output [
  {
    name: "OUTPUT"
    data_type: TYPE_FP32
    dims: [ 1000 ]
  }
]
ensemble_scheduling {
  step [
    {
      model_name: "image_preprocess_nchw_3x224x224_inception"
      model_version: -1
      input_map {
        key: "INPUT"
        value: "INPUT"
      }
      output_map {
        key: "OUTPUT"
        value: "preprocessed_image"
      }
    },
    {
      model_name: "resnet50_netdef"
      model_version: -1
      input_map {
        key: "gpu_0/data"
        value: "preprocessed_image"
      }
      output_map {
        key: "gpu_0/softmax"
        value: "OUTPUT"
      }
    }
  ]
}
